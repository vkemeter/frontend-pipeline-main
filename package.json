{
  "name": "frontend-pipeline-main",
  "version": "1.3.8",
  "description": "A Frontend Pipeline Main Package. Require this in Your package.json and your are ready to go.",
  "engines": {
    "node": ">=14.15.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "main": "index.js",
  "dependencies": {
    "@agostone/gulp-glass": "https://github.com/vkemeter/gulp-glass.git",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "8",
    "backstopjs": "^5.1.0",
    "bootstrap": "^4.5.3",
    "browserify": "^17.0.0",
    "del": "^6.0.0",
    "family.scss": "^1.0.8",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-concat": "^2.6.1",
    "gulp-dart-sass": "^1.0.2",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-sass-glob": "^1.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-typescript": "^6.0.0-alpha.1",
    "gulp-uglify": "^3.0.2",
    "jquery": "^3.5.1",
    "js-yaml": "^4.0.0",
    "jsonschema": "^1.4.0",
    "lodash": "^4.17.21",
    "read-data": "^1.1.0",
    "sassdash": "^0.9.0",
    "tsify": "^5.0.2",
    "typescript": "^4.2.4",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "write": "^2.0.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.168",
    "@types/node": "^15.0.1",
    "jest": "^26.6.3",
    "mock-fs": "^4.13.0",
    "ts-node": "^9.1.1",
    "typescript-json-schema": "^0.50.0"
  },
  "scripts": {
    "presets": "cp -Rvn presets/* ../../",
    "test": "jest ./tests/unit/.*.spec.js",
    "build:schema:legacy": "typescript-json-schema --out ./src/schemas/config.json --strictNullChecks --required ./src/types/*.ts *",
    "build:schema": "ts-node ./build/generateJsonSchemas.ts",
    "build:ts": "tsc",
    "build": "npm run build:schema && npm run build:ts",
    "watch": "tsc --watch"
  },
  "author": "Volker Kemeter",
  "license": "MIT"
}
