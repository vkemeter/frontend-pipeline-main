{
  "name": "frontend-pipeline-main",
  "version": "1.3.8",
  "description": "A Frontend Pipeline Main Package. Require this in Your package.json and your are ready to go.",
  "engines": {
    "node": ">=14.15.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "main": "index.js",
  "dependencies": {
    "gulp-imagemin": "^7.1.0",
    "lodash": "^4.17.21",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@types/gulp": "^4.0.8",
    "@types/gulp-imagemin": "^7.0.2",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.170",
    "@types/mock-fs": "^4.13.0",
    "@types/node": "^15.0.1",
    "jest": "^26.6.3",
    "mock-fs": "^4.13.0",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "typescript-json-schema": "^0.50.0"
  },
  "scripts": {
    "presets": "cp -Rvn presets/* ../../",
    "test": "jest ./test/unit/.*.spec.ts",
    "build:schema:legacy": "typescript-json-schema --out ./src/schemas/config.json --strictNullChecks --required ./src/types/*.ts *",
    "build:schema": "ts-node ./build/generateJsonSchemas.ts",
    "build:ts": "tsc",
    "build": "npm run build:schema && npm run build:ts",
    "watch": "tsc --watch"
  },
  "author": "Volker Kemeter",
  "license": "MIT"
}
